{
  "name": "vsharp-cli",
  "version": "1.5.0",
  "description": "A simple CLI for scaffolding Vue.js projects.",
  "main": "lib/index.js",
  "bin": {
    "vsharp": "bin/vsharp",
    "vsharp-init": "bin/vsharp-init",
    "vsharp-list": "bin/vsharp-list"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "lint": "eslint test lib bin/* --env mocha",
    "e2e": "rimraf test/e2e/mock-template-build/*.* && BABEL_ENV=development mocha test/e2e/test.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liaohongdong/vsharp-cli.git"
  },
  "keywords": [
    "vue",
    "cli",
    "spa"
  ],
  "author": "liaohongdong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liaohongdong/vsharp-cli/issues"
  },
  "homepage": "https://github.com/liaohongdong/vsharp-cli#readme",
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "consolidate": "^0.16.0",
    "download-git-repo": "^0.1.1",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "metalsmith": "^2.3.0",
    "read-metadata": "^1.0.0",
    "request": "^2.67.0",
    "rimraf": "^2.5.0",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  }
}
